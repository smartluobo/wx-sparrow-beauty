.address {
    position: relative;
    height: 165rpx;
}

.default_box {
    position: absolute;
    transform: translate(-50%, -50%);
    top: 50%;
    left: 50%;
}

.default {
    color: #FF5E6B;
    font-size: 30rpx;
    text-align: center;
}

.img_box {
    position: absolute;
    right: 0;
    top: 0;
    height: 5.15625rem;
    width: 1.90625rem;
    text-align: center;
}

.site image {
    height: 28rpx;
    width: 15rpx;
    margin-top: 70rpx;
}

.site {
    height: 165rpx;
    border-bottom: 20rpx solid #f5f5f5;
    position: relative;
}

.overseasGo {
    height: 90rpx;
    background: #e3f0ff;
    font-size: 24rpx;
    color: #666;
    padding: 14rpx 30rpx 16rpx 30rpx;
    line-height: 32rpx;
}

.userName {
    border-bottom: 1rpx solid #E6E6E6;
    line-height: 104rpx;
    padding-left: 30rpx;
    padding-right: 24rpx;
    font-size: 28rpx;
}

.userName input {
    height: 104rpx;
    float: right;
    text-align: right;
    color: #333;
}

.box {
    padding-top: 40rpx;
    padding-left: 30rpx;
    font-size: 32rpx;
}

.address_info {
    font-size: 32rpx;
    padding-top: 10rpx;
    padding-left: 26rpx;
    display: inline-block;
    width: 650rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.tell {
    padding-left: 48rpx;
}

.activity_s {
    height: 70rpx;
}

.activityTagSpan {
    display: inline-block;
    font-size: 24rpx;
    border-radius: 5rpx;
    color: #fff;
    width: auto;
    height: 30rpx;
    background: #FF8587;
    line-height: 30rpx;
    padding: 0 5rpx;
    margin-top: 20rpx;
    margin-left: 5%;
    float: left;
}

.activityTag2 {
    width: 83%;
    height: 70rpx;
    float: left;
}

.activityBox {
    padding-left: 30rpx;
    width: 55%;
    font-size: 26rpx;
    color: #FF8587;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    line-height: 70rpx;
    float: left;
}

.content_b {
    border-bottom: 20rpx #f5f5f5 solid;
}

.footer {
    height: 98rpx;
    background: #fff;
    bottom: 0;
    position: fixed;
    z-index: 10;
    left: 0;
    right: 0;
    border-top: 1rpx #E6E6E6 solid;
}

.footer_txt {
    margin-left: 30rpx;
    line-height: 98rpx;
    font-size: 28rpx;
    color: #333;
    float: left;
}

.price {
    font-size: 1rem;
    color: #FF5E6B;
    margin-left: 25rpx;
    float: left;
    line-height: 98rpx;
}

.btn_box {
    margin-right: 0rem;
    position: absolute;
    right: 0;
}

.btn {
    width: 250rpx;
    height: 98rpx;
    background: #FF5E6B;
    color: white;
    font-size: 32rpx;
    line-height: 98rpx;
    text-align: center;
}

.item_box_s {
    height: auto;
    border-bottom: 1rpx solid #e6e6e6;
    position: relative;
    padding-bottom: 15rpx;
}

.item_box {
    height: auto;
    min-height: 180rpx;
    position: relative;
}

.item {
    overflow: hidden;
    height: auto;
    position: relative;
}

.img_big {
    margin-top: 30rpx;
    position: absolute;
    left: 30rpx;
    top: 0;
    height: auto;
    zoom: 1;
}

.img_big image {
    width: 120rpx;
    height: 120rpx;
    min-width: 120rpx;
    object-fit: cover;
}

.center {
    padding-left: 180rpx;
    overflow: hidden;
    padding-bottom: 30rpx;
}

.title {
    height: 26rpx;
    margin-top: 30rpx;
}

.title text {
    display: block;
    color: #333;
    font-size: 26rpx;
    width: 90%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.genre_box {
    overflow: hidden;
    width: 570rpx;
}

.genre {
    line-height: 50rpx;
    height: 50rpx;
    font-size: 22rpx;
    color: #666;
    width: 400rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    float: left;
}

.count {
    font-size: 28rpx;
    margin-right: 24rpx;
    margin-top: 17rpx;
    float: right;
}

.qian {
    padding-top: 24rpx;
    font-size: 28rpx;
}

.qian_left {}

.item_two {
    height: auto;
    min-height: 88rpx;
    /* width: 100%; */
    border-bottom: #E6E6E6 solid 1rpx;
    line-height: 88rpx;
    font-size: 28rpx;
    padding-left: 30rpx;
    position: relative;
}

.compile {
    float: right;
}

.compile_txt {
    color: #999;
}

.icon {
    display: block;
    width: 1.90625rem;
    height: 2.75rem;
    text-align: center;
    margin-right: 0.375rem;
    float: right;
}

.icon image {
    height: 26rpx;
    width: 13rpx;
    margin-top: 31rpx;
}

.wbk {
    position: absolute;
    right: 5rpx;
    top: 0;
    height: 88rpx;
    width: 630rpx;
    font-size: 28rpx;
}

.cost_list {
    border-top: 20rpx #f5f5f5 solid;
    padding: 15rpx 24rpx 15rpx 30rpx;
}

.list {
    font-size: 28rpx;
    color: #333;
    height: 58rpx;
    line-height: 58rpx;
}

.order {
    padding-bottom: 98rpx;
}

.list_right {
    float: right;
}

.combinationBox {
    height: 130rpx;
    /* width: 100%; */
    position: relative;
}

.combinationImg {
    position: absolute;
    top: 15rpx;
    left: 30rpx;
}

.combinationImg image {
    width: 100rpx;
    height: 100rpx;
}

.combinationContent {
    padding-left: 160rpx;
    height: 130rpx;
    padding-right: 24rpx;
}

.combinationTitle {
    font-size: 26rpx;
    color: #333;
    width: 550rpx;
    display: block;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.combinationMiddle {
    overflow: hidden;
    height: 52rpx;
    line-height: 52rpx;
}

.combinationPrice {
    font-size: 22rpx;
}

.combinationSku {
    float: left;
    font-size: 22rpx;
    color: #666;
}

.c_couponPrice {
    color: #FF5E6B;
    display: inline-block;
    padding-left: 40rpx;
}

.complimentaryText {
    /* width: 100%; */
    padding-left: 130rpx;
    padding-right: 70rpx;
    height: 130rpx;
    line-height: 130rpx;
    font-size: 26rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.complimentaryCount {
    position: absolute;
    top: 0;
    right: 24rpx;
    font-size: 26rpx;
    height: 130rpx;
    line-height: 130rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.activityBtn {
    float: right;
    width: 13%;
    color: #FF8587;
    font-size: 24rpx;
    line-height: 100rpx;
    text-align: right;
}

.activityBtn text {
    float: right;
    display: inline-block;
    width: 13rpx;
    height: 72rpx;
    background: url(http://image.domolife.cn/wechatIcon/RED@2x.png) no-repeat center;
    background-size: 13rpx 26rpx;
    margin-right: 1rem;
}