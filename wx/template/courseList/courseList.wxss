.page {
    overflow: hidden;
}

.goodsBox {
    padding: 5rpx;
    overflow: hidden;
    background: #f5f5f5;
    border-left: 10rpx solid #f5f5f5;
}

.goodsList {
    float: left;
    margin-top: 10rpx;
    width: 50%;
}

.goods {
    background: #fff;
    width: 355rpx;
}

.goodsNot {
    width: 174rpx;
    height: 174rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    margin: -90.5rpx 0 0 -90.5rpx;
}

.goodsImg {
    height: 355rpx;
    width: 355rpx;
}

.goodsImgBox {
    position: relative;
}

.goodsText {
    background: #fff;
    width: 335rpx;
    height: 200rpx;
    margin-top: -10rpx;
    padding: 0 10rpx;
}

.goodsTitle {
    font-size: 24rpx;
    line-height: 44rpx;
    /* padding-top: 8rpx; */
    color: #333;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.goodsSubTitle {
    font-size: 22rpx;
    line-height: 42rpx;
    padding-bottom: 6rpx;
    color: #999;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.price_goods {
    font-size: 26rpx;
    color: #ff5e6b;
}

.activityTag {
    padding: 0 5rpx;
    color: #fff;
    width: auto;
    height: 32rpx;
    background: #ff8587;
    border-radius: 5rpx;
    font-size: 0.6875rem;
    text-align: center;
    line-height: 32rpx;
    float: left;
    margin-top: 6rpx;
    margin-right: 10rpx;
}

.labelTag {
    padding: 0 5rpx;
    color: #fff;
    width: auto;
    height: 32rpx;
    background: #ffb81e;
    border-radius: 5rpx;
    font-size: 0.6875rem;
    text-align: center;
    line-height: 32rpx;
    float: left;
    margin-top: 6rpx;
    margin-right: 10rpx;
}

.goodsName {
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.activityTagBox {
    padding-top: 16rpx;
    height: 46rpx;
}